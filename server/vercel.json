{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node@latest"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/fetch-pdf",
            "dest": "/api/fetch-pdf"
        },
        {
            "src": "/create-pdf",
            "dest": "/api/create-pdf"
        }
    ]
}