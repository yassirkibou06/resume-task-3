{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
                "Access-Control-Allow-Credentials": "true"
            },
            "continue": true
        },
        {
            "src": "(.*)",
            "methods": ["OPTIONS"],
            "dest": "index.js"
        },
        {
            "src": "/create-pdf",
            "methods": ["POST"],
            "dest": "/index.js"
        },
        {
            "src": "/fetch-pdf",
            "methods": ["GET"],
            "dest": "/index.js"
        }
    ]
}